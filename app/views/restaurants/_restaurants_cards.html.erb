
<div class="container mt-4">
  <% restaurants.each do |restaurant| %>
  <div class="card mt-4">
    <div class="card-body">
      <h1 class="card-title"><%= restaurant.name %></h1>
      <h4><%= restaurant.address %></h4>
      <p class="card-text"><%= restaurant.phone_number %></p>
      <p class="card-text"><%= restaurant.category %></p>
    </div>
  </div>
  <% end %>
</div>
